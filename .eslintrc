{
  "extends": [
    "eslint:recommended"
  ],
  "plugins": [
    "html"
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "env": {
      "es6": true,
      "node": true,
      "jquery": true,
      "browser": true
  },
  "rules": {
    "no-unused-vars": "off"
  }
}